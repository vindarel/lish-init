
(format t "new command: fip~&")
(lish:defcommand fip
  ()
  "Listen FIP."
  (uiop:run-program (list "mpv"
                          "http://direct.fipradio.fr/live/fip-midfi.mp3")
                    :output t
                    :input :interactive))
(lish:defcommand culture
  ()
  "Listen France Culture."
  (uiop:run-program (list "mpv"
                          "http://direct.franceculture.fr/live/franceculture-midfi.mp3")
                    :output t
                    :input :interactive))
